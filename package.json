{
  "name": "cypress-vite-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src/**",
    "coverage": "nyc report",
    "cypress:open": "CYPRESS_BASE_URL='http://localhost:3000' cypress open --e2e --browser chrome",
    "cypress:run": "CYPRESS_BASE_URL='http://localhost:3000' cypress run --e2e --browser chrome",
    "cy:e2e": "start-server-and-test start http-get://localhost:3000 cypress:run && npm run coverage",
    "cy:e2e-debug": "start-server-and-test start http-get://localhost:3000 cypress:open"
  },
  "dependencies": {
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.11.16",
    "@mui/styled-engine-sc": "^5.11.11",
    "dotenv": "^16.0.3",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "styled-components": "^5.3.9",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.3",
    "@cypress/react18": "^2.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "cypress": "10.9",
    "cypress-vite": "^1.3.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "i": "^0.3.7",
    "npm": "^9.6.4",
    "nyc": "^15.1.0",
    "prettier": "^2.8.7",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-istanbul": "^4.0.1"
  }
}
